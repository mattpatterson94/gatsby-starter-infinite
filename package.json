  "keywords": [
    "gatsby"
  ],
  "overrides": {
    "gatsby-plugin-scroll-reveal": {
      "react": "^17.0.1"
    }
  },
  "resolutions": {
    "gatsby-plugin-scroll-reveal/react": "^17.0.1"
  },
  "license": "0BSD",
  "scripts": {
    "codegen": "graphql-codegen --config codegen.yaml",
    "codegen:watch": "graphql-codegen --config codegen.yaml --watch",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "lint": "eslint src/**/*.{ts,tsx}",
    "lint:fix": "eslint --fix src/**/*.{ts,tsx}",
    "start": "concurrently \"yarn:codegen:watch\" \"yarn:proxy\" \"yarn:develop\"",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "proxy": "netlify-cms-proxy-server",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
