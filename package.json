    "prop-types": "^15.8.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-google-recaptcha": "^2.1.0",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.4.0",
    "remark": "^14.0.2",
    "remark-html": "^15.0.1",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.5.5"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.6.4",
    "@graphql-codegen/typescript": "^2.4.11",
    "@graphql-codegen/typescript-operations": "^2.4.0",
